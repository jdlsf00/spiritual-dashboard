# Production environment variables for Spiritual Dashboard React
REACT_APP_API_BASE_URL=http://localhost:8001
REACT_APP_TAROT_SERVICE_URL=http://localhost:7870
REACT_APP_ASTROLOGY_SERVICE_URL=http://localhost:7871
REACT_APP_EBOOK_SERVICE_URL=http://localhost:7872
REACT_APP_WEBSOCKET_URL=ws://localhost:8001/ws

# Feature flags
REACT_APP_ENABLE_REAL_TIME=true
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_NOTIFICATIONS=true

# Theme and UI
REACT_APP_DEFAULT_THEME=dark
REACT_APP_ANIMATION_DURATION=300

# API Configuration
REACT_APP_API_TIMEOUT=10000
REACT_APP_CACHE_DURATION=300000

# Environment
NODE_ENV=production
GENERATE_SOURCEMAP=false
